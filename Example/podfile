platform :ios, '10.3'
use_frameworks!

target 'iOSExample' do
	# This will use the version of the Apptentive SDK included in this repository.
	pod 'apptentive-ios', :path => '..'

	# In your own apps, you will want to use the latest version from CocoaPods, like so:
	# pod 'apptentive-ios', '~> 5'
end
